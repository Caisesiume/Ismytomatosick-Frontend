{
    "version": 2,
    "builds": [{
            "src": "Tomato Diagnostics/core/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [{
        "src": "/(.*)",
        "dest": "Tomato Diagnostics/core/wsgi.py"
    }]
}