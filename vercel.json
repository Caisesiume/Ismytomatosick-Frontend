{
    "version": 2,
    "builds": [{
            "src": "requirements.txt",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb"
            }
        },
        {
            "src": "Tomato Diagnostics/core/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb"
            }
        }
    ],
    "routes": [{
        "src": "/(.*)",
        "dest": "Tomato Diagnostics/core/wsgi.py"
    }]
}